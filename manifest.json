{
	"action": {
		"default_popup": "popup.html",
		"default_icon": "images/video-play-128.png"
	},
	"background": {
		"service_worker": "service-worker.js"
	},
	"content_scripts": [
		{
			"matches": [
				"https://555pian.com/*",
				"https://v.qq.com/*",
				"https://www.iqiyi.com/*",
				"https://www.bilibili.com/*",
				"https://v.youku.com/*",
				"https://www.youtube.com/*"
			],
			"js": [
				"content-script.js"
			]
		}
	],
	"description": "用来控制网页视频播放器在指定位置快进一定时长。",
	"key": "",
	"manifest_version": 3,
	"name": "Video Controller",
	"permissions": [
		"storage"
	],
	"sandbox": {
		"pages": [
			"rule/rule.html"
		]
	},
	"version": "1.0"
}